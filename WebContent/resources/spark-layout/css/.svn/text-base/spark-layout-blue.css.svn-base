.irA {
	border: none;
	text-align: center;
	transition: all 0.5s;
	cursor: pointer;
	color: white !important;
	background: linear-gradient(-45deg, #fbfcfd, #3e85a9, #99d5ec, #c5dae2);
	background-size: 400% 400%;
	-webkit-animation: Gradient 15s ease infinite;
	-moz-animation: Gradient 15s ease infinite;
	animation: Gradient 15s ease infinite;
	-webkit-box-shadow: 0px 9px 13px 3px rgba(0, 0, 0, 0.73);
	-moz-box-shadow: 0px 9px 13px 3px rgba(0, 0, 0, 0.73);
	box-shadow: 0px 9px 13px 3px rgba(0, 0, 0, 0.73);
	border-radius: 15px 50px;
}

.irA span {
	cursor: pointer;
	display: inline-block;
	position: relative;
	transition: 0.5s;
}

.irA span:after {
	content: '\00bb';
	position: absolute;
	opacity: 0;
	top: 0;
	right: -20px;
	transition: 0.5s;
}

.irA:hover span {
	padding-right: 25px;
}

.irA:hover span:after {
	opacity: 1;
	right: 0;
}

