/*****CONTROL TROGLE DE BOTON*****/
#toggle {
	position: relative;
	appearance: none;
	cursor: pointer;
	left: -100%;
	top: -100%;
	width: 0px;
}

#toggle+label {
	width: 120px !important;
	cursor: pointer;
	background: #dd6149;
	border-radius: 3px;
	padding: 8px 10px;
	color: #FFF;
	line-height: 20px;
	font-size: 12px;
	text-align: center;
	-webkit-font-smoothing: antialiased;
	cursor: pointer;
	transition: all 500ms ease;
}

#toggle+label:after {
	content: "Cerrar"
}

#toggle:checked ~ .message {
	top: 0;
}

#toggle:checked ~ .container {
	margin-top: 0px !important;
}

#toggle:checked+label {
	background: #dd6149;
}

#toggle:checked+label:after {
	content: "Cerrar"
}

