html{
	min-height: 101%;
}

body{
	font-family: Arial,Verdana,sans-serif !important;
	font-size: 0.9em;
        background-image: -moz-linear-gradient(top, #f0f0f0, #707070); /* FF3.6 */
        background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0, #707070), color-stop(1, #f0f0f0)); /* Safari 4+, Chrome 2+ */
        background-image: -webkit-linear-gradient(top, #f0f0f0, #707070); /* Safari 5.1+, Chrome 10+ */
        background-image: -o-linear-gradient(top, #f0f0f0, #707070); /* Opera 11.1+ */
        background-image: -ms-linear-gradient(top, #f0f0f0, #707070); /* IE10+ */
        filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#f0f0f0', endColorstr='#707070'); /* IE6,IE7 */
        -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#f0f0f0', endColorstr='#707070')"; /* IE8+ */
        background-image: linear-gradient(top, #f0f0f0, #707070);
}

#idBodySII{
	margin: 1% auto 2% auto !important;
	padding: 5px 5px !important;
	-webkit-border-radius: 10px 11px 12px 13px;
	-moz-border-radius: 10px 11px 12px 13px;
	border-radius: 10px 11px 12px 13px;

	border: 1px solid #ccc;
	width:1500px;
	min-width:1500px;
	min-height: 600px;

	background: #fff;

	-webkit-box-shadow: #666 10px 10px 10px 3px;
	-moz-box-shadow: #666 10px 10px 10px 3px; /* color, h-offset, v-offset, blur, spread */
	box-shadow: #666 10px 10px 10px 3px; /* inset is optional */

}

table.clsTablePrincipal{
	margin: 5px auto !important;
    	padding: auto;

    	width: 100% !important;
}

#idHeadSII{
	background-image: none;// !important;
	background: no-repeat;
	height: 100px !important;
	background-position: 35px 8px;
	border:0px solid #ccc;
	margin: 1px 0 5px 0;
	padding: 1px 1px 10px 1px;
	border-bottom:10px solid #BED6F8;
}

#idCuerpoSII{
	display:block;
	width:100% !important;
	margin:0px;
	padding: 0 auto;
	border: 0px solid #ccc;
	float:left !important;
}

#idCuerpoSII .clsIzquierdo{
	display:block;
	border: 0px !important;
	width: 21% !important;
	margin: 0 1px 0 0 !important;
	min-height:600px;

	background-image: -moz-linear-gradient(top, #33AFDE, #fff); /* FF3.6 */
	background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0, #fff), color-stop(1, #33AFDE)); /* Safari 4+, Chrome 2+ */
	background-image: -webkit-linear-gradient(top, #33AFDE, #fff); /* Safari 5.1+, Chrome 10+ */
	background-image: -o-linear-gradient(top, #33AFDE, #ffffff); /* Opera 11.1+ */
	background-image: -ms-linear-gradient(top, #33AFDE, #ffffff); /* IE10+ */
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#BED6F8', endColorstr='#ffffff'); /* IE6,IE7 */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#BED6F8', endColorstr='#ffffff')"; /* IE8+ */
	background-image: linear-gradient(top, #BED6F8, #ffffff);
	font-size:0.9em;
	font-family: Arial,Verdana,sans-serif !important;

        -webkit-border-radius: 10px 11px 12px 13px;
        -moz-border-radius: 10px 11px 12px 13px;
        border-radius: 10px 11px 12px 13px;
}

#idCuerpoSII div.rich-tree{
	padding: 19px 0 0 -3px;
} 

#idCuerpoSII .clsIzquierdo a:link,
#idCuerpoSII .clsIzquierdo a:visited{
	font-family: Arial,Verdana,sans-serif !important;
	font-size:0.9em !important;
	color:#555 !important;
	text-decoration:none;
}

#idCuerpoSII .clsIzquierdo a:hover{
	color:#000 1important;
	text-decoration: underline;
}

#idCuerpoSII .clsCentro{
	display:block;
	width: 78%;
	border: 0px solid #ccc;
	margin:0px !important;
	padding: 0 0 0 1px;
}

form#j_id374{
