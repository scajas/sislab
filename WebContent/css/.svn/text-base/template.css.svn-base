        background-image: -ms-linear-gradient(top, #fff, #BED6F8); /* IE10+ */
        filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#fff', endColorstr='#BED6F8'); /* IE6,IE7 */
        -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#fff', endColorstr='#BED6F8')"; /* IE8+ */
        background-image: linear-gradient(top, #fff, #BED6F8);
	
}


div.clsInfoUsuario a:link,
div.clsInfoUsuario a:visited,
div.clsInfoUsuario a:hover{
