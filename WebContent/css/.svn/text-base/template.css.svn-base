.clsInfoUsuario{
	color:#081D49;
	font-weight:bold !important;
	text-align:center !important;
	margin: 53px 0 0 0 !important;
	padding: 4px 18px;

	border-top:1px solid #BED6F8;
	border-left:1px solid #BED6F8;
	border-right:1px solid #BED6F8;
	border-bottom:0px solid #BED6F8;

