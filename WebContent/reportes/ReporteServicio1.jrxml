	<parameter name="idServicio" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT s.id_servicio,
ts.nombre_ts,
l.nombre_l,
s.nombre_s,
s.precio_s
FROM "Laboratorios".tiposervicio ts,
"Laboratorios".laboratorio_usuario lu,
"Laboratorios".laboratorio l,
"Laboratorios".servicio s,
"public".usuario us
WHERE lu.id_laboratorio=l.id_laboratorio
and s.id_laboratorio= l.id_laboratorio
and lu.id_usuario=us.id_usuario
and lu.id_usuario =1
and us.id_unidad=1
and ts.id_tiposerv= $P{idServicio}]]>
