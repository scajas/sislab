	<parameter name="fechaInicio" class="java.lang.String"/>
	<parameter name="fechaFin" class="java.lang.String"/>
	<parameter name="idUnidad" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT c.id_compra,
c.fecha_co,
c.descr_compra,
pv.nombre_pv,
c.monto_co
from "Laboratorios".compra c,
"Laboratorios".proveedor pv
where
c.id_proveedor = pv.id_proveedor
AND c.fecha_co BETWEEN $P{fechaInicio} AND $P{fechaFin}
AND c.id_unidad=$P{idUnidad}
ORDER BY c.fecha_co ASC]]>
	</queryString>
