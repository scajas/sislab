	<parameter name="tipoCliente" class="java.lang.String"/>
	<parameter name="estadoProforma" class="java.lang.String"/>
	<queryString>
		<![CDATA[select p.id_proforma,
c.nombre_cl,
p.fecha,
p.estado_po,
p.subtotal_po,
p.iva_po,
p.total_po
from "Laboratorios".proforma p,
"Laboratorios".cliente c,
"public".usuario us,
"Laboratorios".tipocliente tc
where p.id_cliente = c.id_cliente
AND c.id_tipocliente=tc.id_tipocliente
AND p.id_usuario=us.id_usuario
AND p.fecha BETWEEN $P{fechaInicio} AND $P{fechaFinal}
AND tc.id_tipocliente=$P{tipoCliente}
AND p.estado_po=$P{estadoProforma}
AND p.id_usuario =648
AND us.id_unidad=2
ORDER BY p.fecha ASC
]]>
	</queryString>
